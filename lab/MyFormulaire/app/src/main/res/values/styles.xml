<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="style_txv" parent="@style/TextAppearance.Compat.Notification.Title">
        <item name="android:color">@color/colorBlue</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textSize">18sp</item>
        <item name="android:typeface">serif</item>
    </style>
    <style name="style_lblresult" parent="@style/TextAppearance.Compat.Notification.Title">
        <item name="android:color">@color/colorAccent</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>

    </style>


</resources>
